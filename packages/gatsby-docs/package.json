{
  "name": "@upgrade-helper/gatsby-docs",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Builds the 'docs' folder as a Gatsby site.",
  "scripts": {
    "convert-docs": "node convert-docs-run.js",
    "test": "jest",
    "gatsby:build": "yarn convert-docs && gatsby build",
    "gatsby:clean": "gatsby clean",
    "gatsby:dev": "yarn convert-docs && gatsby develop",
    "gatsby:serve": "gatsby serve",
    "gatsby:conditional-deploy": "node gatsby-conditional-deploy-cli.js",
    "gatsby:deploy": "yarn convert-docs && gatsby build --prefix-paths && gh-pages -d public -b gh-pages -r https://${GH_TOKEN}@github.com/IBM/spm-ui-upgrade-helper.git"
  },
  "devDependencies": {
    "commander": "^8.1.0",
    "gatsby": "^3.10.2",
    "gatsby-theme-carbon": "^2.0.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shelljs": "^0.8.4"
  }
}
