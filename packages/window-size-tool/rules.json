[
  {
    "width": "> 768",
    "terms": [
      "/*/CLUSTER/@NUM_COLS = 2",
      "count(/*/LIST) > 0 and count(/*/LIST/*) < 5"
    ],
    "target": "md"
  },
  {
    "width": "> 576 and <= 768",
    "terms": [
      "count(/*/CLUSTER[not(@NUM_COLS)]) > 0 or /*/CLUSTER/@NUM_COLS = 1",
      "count(/*/LIST) > 0 and count(/*/LIST/*) <= 2"
    ],
    "target": "sm"
  }
]
