<?xml version="1.0" encoding="UTF-8"?>
<!--
  Expect width=600 to change to width=1000.
-->
<PAGE PAGE_ID="realistic-file-no-changes" WINDOW_OPTIONS="width=600,height=300" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="CuramUIMSchema">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="PersonAdmin" NAME="DISPLAY1" OPERATION="read" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="PersonAdmin" NAME="ACTION1" OPERATION="modify" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="MockProcess" NAME="ACTION2" OPERATION="insert" PHASE="ACTION"/>

  <CONNECT>
    <SOURCE NAME="CONSTANT" PROPERTY="testPersonID"/>
    <TARGET NAME="DISPLAY1" PROPERTY="key$identifier"/>
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="CONSTANT" PROPERTY="testPersonID"/>
    <TARGET NAME="ACTION1" PROPERTY="details$identifier"/>
  </CONNECT>

  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL LABEL="Button.Submit" TYPE="SUBMIT"/>
  </ACTION_SET>

  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Input1" LABEL_WIDTH="50">
    <CONTAINER LABEL="Container.Label.Date">
      <FIELD LABEL="Field.Label.Date" WIDTH="50">
        <CONNECT>
          <SOURCE NAME="DISPLAY1" PROPERTY="dateOfBirth"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION1" PROPERTY="dateOfBirth"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Checkbox">
        <CONNECT>
          <TARGET NAME="ACTION2" PROPERTY="bool"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>

  <CLUSTER NUM_COLS="3" TITLE="Cluster.Title.Input2">
    <CONTAINER LABEL="Container.Label.Date">
      <FIELD LABEL="Field.Label.Date" WIDTH="25">
        <CONNECT>
          <SOURCE NAME="DISPLAY1" PROPERTY="dateOfBirth"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION1" PROPERTY="dateOfBirth"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Checkbox">
        <CONNECT>
          <TARGET NAME="ACTION2" PROPERTY="bool"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>

  <CLUSTER NUM_COLS="1" TITLE="Cluster.Title.Input3">
    <CONTAINER LABEL="Container.Label.Date">
      <FIELD LABEL="Field.Label.Date" WIDTH="60">
        <CONNECT>
          <SOURCE NAME="DISPLAY1" PROPERTY="dateOfBirth"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION1" PROPERTY="dateOfBirth"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Checkbox">
        <CONNECT>
	        <TARGET NAME="ACTION2" PROPERTY="bool"/>
	      </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>

  <CLUSTER NUM_COLS="1" TITLE="Cluster.Title.Input4">
    <FIELD LABEL="Field.Label.Date">
      <CONNECT>
        <SOURCE NAME="DISPLAY1" PROPERTY="dateOfBirth"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION1" PROPERTY="dateOfBirth"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Checkbox">
      <CONNECT>
        <TARGET NAME="ACTION2" PROPERTY="bool"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>