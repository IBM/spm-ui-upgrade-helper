<!--
  - Expected: Width changes to 1000 (large).
  -
  - Reason: PAGE element WINDOW_OPTIONS attribute width is equal to or greater 
  - than 768 with a nested 2 of more column CLUSTER that contains nested LISTs.
  -->
<?xml version="1.0" encoding="UTF-8"?>
<PAGE WINDOW_OPTIONS="width=1000">
  <CONTAINER>
    <CLUSTER NUM_COLS="2">
    <CONTAINER>
        <LIST>
          <FIELD/>
        </LIST>
      </CONTAINER>
      <CONTAINER>
        <LIST>
          <FIELD/>
        </LIST>
      </CONTAINER>
    </CLUSTER>
  </CONTAINER>
</PAGE>