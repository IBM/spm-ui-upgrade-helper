{
  "name": "spm-ui-upgrade-helper",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "",
  "author": "",
  "scripts": {
    "echo-commit": "node echo-commit.js",
    "build:dev": "yarn echo-commit && yarn docker-tasks build -p --build-arg show_all_tools=true",
    "build:release": "yarn echo-commit && yarn docker-tasks build -p",
    "at:build": "node acceptance-tests/scripts/generate-acceptance-test-data.js",
    "install-all": "npm-run-all --sequential install:*",
    "install:browser-app": "cd packages/browser-app && yarn install",
    "install:code-generator": "cd packages/code-generator && yarn install",
    "install:css-rules-engine": "cd packages/css-rules-engine && yarn install",
    "install:icon-replacer": "cd packages/icon-replacer && yarn install",
    "install:initial-check": "cd packages/initial-check && yarn install",
    "install:js-rules-engine": "cd packages/js-rules-engine && yarn install",
    "install:shared-utils": "cd packages/shared-utils && yarn install",
    "start": "lerna run start --scope @upgrade-helper/initial-check && lerna run start --scope @upgrade-helper/browser-app --scope @upgrade-helper/css-rules-engine --scope @upgrade-helper/js-rules-engine",
    "generate-files": "node ./packages/code-generator/index.js",
    "test": "lerna run test --ignore @upgrade-helper/skeleton --ignore @upgrade-helper/icon-replacer && yarn test-release-script",
    "test-release-script": "jest ./release/.*.test.js",
    "release": "node release/release-cli.js"
  },
  "resolutions": {
    "@types/react": "^16.8.0"
  },
  "devDependencies": {
    "commander": "^8.0.0",
    "docker-tasks": "^0.1.49",
    "eslint": "^7.20.0",
    "jest": "^27.0.6",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5"
  }
}
