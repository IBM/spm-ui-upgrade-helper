{
  "name": "spm-ui-upgrade-helper",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "",
  "author": "",
  "scripts": {
    "echo-commit": "node echo-commit.js",
    "build:dev": "yarn echo-commit && yarn generate-files --dev && yarn docker-tasks build -p --build-arg dev_mode=true",
    "build:release": "yarn echo-commit && yarn generate-files && yarn docker-tasks build -p",
    "at:build": "node acceptance-tests/scripts/generate-acceptance-test-data.js",
    "at:test": "jest acceptance-tests/puppeteer/.*.test.js",
    "install-all": "yarn install && lerna bootstrap",
    "initial-check": "cd packages/initial-check && yarn start",
    "start": "yarn initial-check && npm-run-all --parallel start:*",
    "start:theia": "node /home/theia/packages/browser-app/src-gen/backend/main.js /home/workspace/output --hostname=0.0.0.0",
    "start:tools": "lerna run --scope @upgrade-helper/*-tool start && yarn start-theia",
    "generate-files": "node ./packages/code-generator/index.js",
    "test": "lerna run test",
    "release": "node packages/release/release-cli.js"
  },
  "resolutions": {
    "@types/react": "^16.8.0"
  },
  "devDependencies": {
    "@folkforms/file-io": "^0.2.3",
    "commander": "^8.0.0",
    "docker-tasks": "^0.1.49",
    "eslint": "^7.20.0",
    "jest": "^27.0.6",
    "puppeteer": "^10.1.0",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5"
  }
}
