{
  "name": "spm-ui-upgrade-helper",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "",
  "author": "",
  "scripts": {
    "echo-commit": "node echo-commit.js",
    "build:dev": "yarn echo-commit && yarn generate-files --dev && yarn docker-tasks build -p --build-arg dev_mode=true",
    "build:release": "yarn echo-commit && yarn generate-files && yarn docker-tasks build -p",
    "at:build": "node acceptance-tests/scripts/generate-acceptance-test-data.js",
    "at:test": "jest acceptance-tests/puppeteer/.*.test.js",
    "install:all": "npm-run-all --sequential install:*",
    "install:code-generator": "cd packages/code-generator && yarn install",
    "install:css-rules-tool": "cd packages/css-rules-tool && yarn install",
    "install:docs": "cd packages/docs && yarn install",
    "install:icon-replacer-tool": "cd packages/icon-replacer-tool && yarn install",
    "install:initial-check": "cd packages/initial-check && yarn install",
    "install:js-rules-tool": "cd packages/js-rules-tool && yarn install",
    "install:main-tool": "cd packages/main-tool && yarn install",
    "install:shared-utils": "cd packages/shared-utils && yarn install",
    "install:show-dev-shortcuts": "cd packages/show-dev-shortcuts && yarn install",
    "install:window-size-tool": "cd packages/window-size-tool && yarn install",
    "initial-check": "cd packages/initial-check && yarn start",
    "start-theia": "node /home/theia/packages/browser-app/src-gen/backend/main.js /home/workspace/output --hostname=0.0.0.0",
    "start-tools": "lerna run --scope @upgrade-helper/*-tool start && yarn start-theia",
    "start": "yarn initial-check && npm-run-all --parallel start-theia start-tools",
    "generate-files": "node ./packages/code-generator/index.js",
    "test": "lerna run test && yarn test:shared-utils && yarn test:release",
    "test:shared-utils": "cd packages/shared-utils && yarn test",
    "test:release": "jest ./release/.*.test.js",
    "release": "node release/release-cli.js"
  },
  "resolutions": {
    "@types/react": "^16.8.0"
  },
  "devDependencies": {
    "@folkforms/file-io": "^0.2.3",
    "commander": "^8.0.0",
    "docker-tasks": "^0.1.49",
    "eslint": "^7.20.0",
    "jest": "^27.0.6",
    "puppeteer": "^10.1.0",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5"
  }
}
